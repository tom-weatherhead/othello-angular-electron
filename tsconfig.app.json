{
	"extends": "./tsconfig.base.json",
	"compilerOptions": {
		"module": "esnext",
		"outDir": "./dist/out-tsc/app",
		"target": "es2015"
	},
	"files": [
		"src/main.ts",
		"src/polyfills.ts"
	],
	"include": [
		"src/**/*.d.ts"
	],
	"exclude": [
		"src/test.ts",
		"src/**/*.spec.ts",
		"src/**/testing"
	]
}
