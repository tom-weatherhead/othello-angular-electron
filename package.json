		"@angular/platform-browser-dynamic": "^12.1.1",
		"@angular/router": "^12.1.1",
		"hammerjs": "^2.0.8",
		"material-design-icons": "^3.0.1",
		"sass": "^1.35.2",
		"thaw-common-utilities.ts": "^0.0.8",
		"thaw-reversi-engine.ts": "^0.3.0",
		"tslib": "^2.3.0",
