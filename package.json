{
	"name": "othello-angular-electron",
	"version": "0.0.0",
	"description": "An Othello (Reversi) proof-of-concept application built on Angular, Docker, and Electron.",
	"keywords": [
		"Angular",
		"Docker",
		"Electron",
		"Othello",
		"PulseAudio",
		"Reversi",
		"TypeScript"
	],
	"private": false,
	"homepage": "https://github.com/tom-weatherhead/othello-angular-electron#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/tom-weatherhead/othello-angular-electron.git"
	},
	"bugs": {
		"url": "https://github.com/tom-weatherhead/othello-angular-electron/issues"
	},
	"main": "main.js",
	"scripts": {
		"clean-packages": "rm -rf node_modules && rm -f package-lock.json",
		"clean-build-products": "rm -rf coverage && rm -rf dist && rm -rf release && rm -f main.js",
		"clean": "npm run clean-packages && npm run clean-build-products",
		"prettier-format": "prettier --write \"./**/*.js\" \"./**/*.ts\"",
		"prettier-check": "prettier --check \"./**/*.js\" \"./**/*.ts\"",
		"lint": "eslint . --ext .ts",
		"ng-lint": "ng lint",
		"pre-build-tests": "npm run prettier-check && npm run lint && npm run ng-lint",
		"build-electron-main": "tsc -p tsconfig.main.json",
		"build:dev": "ng build -c dev --base-href . && npm run build-electron-main",
		"build:prod": "ng build -c production --base-href . && npm run build-electron-main",
		"electron-with-gpu": "electron .",
		"electron-without-gpu": "electron --disable-gpu .",
		"electron": "npm run electron-with-gpu",
		"electron:build": "npm run clean-build-products && npm run build:prod && electron-builder build",
		"unit-tests": "xvfb-maybe ng test --watch=false --code-coverage",
		"e2e": "cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
		"system-tests": "npm run e2e",
		"test": "npm run pre-build-tests && npm run build:dev && npm run unit-tests && npm run system-tests",
		"re": "npm run clean-build-products && npm run prettier-format && npm test && npm start",
		"all": "npm run clean && npm i && npm test && npm start",
		"f": "npm run clean-packages && npm i && npm run re",
		"serve": "ng serve",
		"start": "npm run electron"
	},
	"config": {},
	"publishConfig": {},
