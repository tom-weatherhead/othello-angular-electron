{
	"name": "othello-angular-electron",
	"description": "An Othello (Reversi) proof-of-concept application built on Angular, Bootstrap, Docker, and Electron.",
	"private": false,
	"version": "0.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/tom-weatherhead/othello-angular-electron.git"
	},
	"homepage": "https://github.com/tom-weatherhead/othello-angular-electron#readme",
	"bugs": {
		"url": "https://github.com/tom-weatherhead/othello-angular-electron/issues"
	},
	"main": "main.js",
	"scripts": {
		"all": "npm run clean && npm i && npm run test-all && npm start",
		"travis": "npm i && npm run test-all",
		"all-notests": "npm run clean && npm i && npm start",
		"clean": "rm -rf coverage && rm -rf dist && rm -rf node_modules && rm -f package-lock.json",
		"build": "npm i && npm start",
		"rebuild": "npm run clean && npm run build",
		"build-ng": "ng build",
		"build-ng-prod": "ng build --prod",
		"electron": "npm run electron-with-gpu",
		"electron-with-gpu": "electron .",
		"electron-without-gpu": "electron --disable-gpu .",
		"lint": "eslint . --ext .ts",
		"ng-lint": "ng lint",
		"prettiercheck": "prettier --check \"./*.js\" \"e2e/**/*.ts\" \"src/**/*.ts\"",
		"format": "prettier --write \"./*.js\" \"e2e/**/*.ts\" \"src/**/*.ts\"",
		"unittests": "ng test --code-coverage",
		"unittests-headless": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
		"e2e": "ng e2e",
		"systemtests": "npm run e2e",
		"test-all": "npm run lint && npm run ng-lint && npm run prettiercheck && npm run unittests-headless && npm run systemtests",
		"serve": "ng serve",
		"start": "npm run build-ng && cp node_modules/bootstrap/dist/js/bootstrap.bundle.min.js.map dist/ && npm run electron"
	},
	"dependencies": {
		"@angular/common": "^10.0.11",
		"@angular/compiler": "^10.0.11",
		"@angular/core": "^10.0.11",
		"@angular/forms": "^10.0.11",
		"@angular/localize": "^10.0.11",
		"@angular/platform-browser": "^10.0.11",
		"@angular/platform-browser-dynamic": "^10.0.11",
		"@angular/router": "^10.0.11",
		"@ng-bootstrap/ng-bootstrap": "^7.0.0",
		"bootstrap": "^4.5.2",
		"electron": "^10.1.7",
		"jquery": "^3.5.1",
		"node-sass": "^4.14.1",
		"popper.js": "^1.16.1",
		"thaw-common-utilities.js": "^0.0.28",
		"thaw-reversi-engine": "^0.2.1",
		"zone.js": "~0.10.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.1000.6",
		"@angular/cli": "^10.0.6",
		"@angular/compiler-cli": "^10.0.11",
		"@types/jasmine": "^3.5.12",
		"@types/jasminewd2": "^2.0.8",
		"@types/jquery": "^3.5.1",
		"@types/node": "^14.6.0",
		"@typescript-eslint/eslint-plugin": "^3.9.1",
		"@typescript-eslint/parser": "^3.9.1",
		"chromedriver": "^84.0.1",
		"codelyzer": "^6.0.0",
		"eslint": "^7.7.0",
		"jasmine-core": "^3.6.0",
		"jasmine-spec-reporter": "^5.0.2",
		"karma": "^5.1.1",
		"karma-chrome-launcher": "^3.1.0",
		"karma-coverage-istanbul-reporter": "^3.0.3",
		"karma-jasmine": "^4.0.1",
		"karma-jasmine-html-reporter": "^1.5.4",
		"prettier": "^2.0.5",
		"protractor": "^7.0.0",
		"selenium-webdriver": "^4.0.0-alpha.7",
		"ts-node": "^8.10.2",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "<4"
	},
	"engines": {
		"node": ">= 10.9"
	},
	"keywords": [
		"Angular",
		"Bootstrap",
		"Docker",
		"Electron",
		"Othello",
		"PulseAudio",
		"Reversi",
		"TypeScript"
	],
	"os": [
		"darwin",
		"linux",
		"win32"
	],
	"cpu": [
		"ia32",
		"x64"
	],
	"contributors": [
		"Autonomous Weapons Systems Unlimited <killer@robots.com>",
		"Buddy Guy <deadbeef@decafbad.gov>",
		"Deprecated <deadc0de@obso1337.org>",
		"Locutus <cube@cyb.org>",
		"Sgt. Pepper <shoot1st@thinkl8r.mil>"
	],
	"author": "Tom Weatherhead <thaw@2hrd4u.org> (https://github.com/tom-weatherhead)",
	"license": "MIT"
}
